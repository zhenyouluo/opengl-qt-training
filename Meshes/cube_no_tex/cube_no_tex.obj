mtllib default.mtl

v -0.5 0.5 -0.5
v -0.5 0.5 0.5
v 0.5 0.5 0.5
v 0.5 0.5 -0.5
v -0.5 -0.5 -0.5
v -0.5 -0.5 0.5
v 0.5 -0.5 0.5
v 0.5 -0.5 -0.5

g cube
usemtl default

f -8 -7 -6
f -8 -6 -5
f -8 -4 -3
f -8 -3 -7
f -6 -2 -1
f -6 -1 -5
f -5 -1 -4
f -5 -4 -8
f -7 -3 -2
f -7 -2 -6
f -3 -4 -1
f -3 -1 -2
